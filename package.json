{
  "name": "tibia-gameplay-tools-app",
  "version": "0.1.0",
  "private": true,
  "description": "A simple app to calculate party shared exp level.",
  "main": "public/electron.js",
  "author": "Michael Anthony Leal Costa",
  "license": "MIT",
  "homepage": "./",
  "repository": "http://github.com/MiCLeal/tibia-gameplay-tools-app",
  "devDependencies": {
    "concurrently": "^3.6.1",
    "electron": "^2.0.7",
    "electron-builder": "^20.28.1",
    "prettier": "^1.14.2",
    "wait-on": "^2.1.0"
  },
  "dependencies": {
    "bootstrap-scss": "^4.1.3",
    "cross-env": "^5.2.0",
    "electron-is-dev": "^0.3.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/styles --include-path ./node_modules src/styles/ -o src/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/styles --include-path ./node_modules src/styles/ -o src/styles --watch --recursive",
    "scss-start": "npm-run-all -p watch-css react-start",
    "build": "yarn build-css && build-react",
    "react-start": "react-scripts start",
    "build-react": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "dev": "concurrently \"cross-env BROWSER=none yarn scss-start\" \"wait-on http://localhost:3000 && electron .\"",
    "e-pack": "build -w -c.extraMetadata.main=build/electron.js",
    "preelectron-pack": "yarn build"
  },
  "build": {
    "publish": {
      "provider": "github"
    }
  }
}
